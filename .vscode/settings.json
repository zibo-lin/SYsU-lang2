{
  "editor.unfoldOnClickAfterEndOfLine": false,

  "files.eol": "\n",
  "files.autoSave": "afterDelay",
  "files.associations": {
    "*.json": "jsonc",
    "TODO": "markdown",
    ".clang-tidy": "yaml",
    "cpp": "cpp",
    "*.l": "lex",
    "*.y": "yacc",
    "CMakeLists.txt": "cmake",
    "forward_list": "cpp",
    "future": "cpp",
    "variant": "cpp",
    "__functional_base": "cpp",
    "__functional_base_03": "cpp",
    "__hash_table": "cpp",
    "__tree": "cpp",
    "__tuple": "cpp",
    "filesystem": "cpp",
    "queue": "cpp",
    "stack": "cpp"
  },
  "flake8.args": ["--ignore=E203,W503", "--max-line-length=88"],
  "autoDocstring.docstringFormat": "one-line-sphinx"
}
